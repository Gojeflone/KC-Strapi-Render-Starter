{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "default": "Blog Title",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "default": "Blog Content Here",
      "required": true
    },
    "BlogPublishDate": {
      "type": "datetime"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "performance",
        "small business",
        "web development",
        "marketing",
        "design"
      ],
      "required": true,
      "default": "web development"
    },
    "HeaderImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
